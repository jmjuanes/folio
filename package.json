{
    "name": "folio",
    "version": "0.9.3",
    "description": "A minimalistic digital whiteboard for sketching and prototyping",
    "private": true,
    "homepage": "https://www.josemi.xyz/folio",
    "repository": "https://github.com/jmjuanes/folio",
    "author": "Josemi Juanes <hello@josemi.xyz>",
    "bugs": "https://github.com/jmjuanes/folio/issues",
    "scripts": {
        "build": "yarn clean && yarn build:app && yarn build:static",
        "build:app": "cross-env NODE_ENV=production webpack build",
        "build:static": "cp static/** ./www/",
        "postbuild:static": "cp ./node_modules/lowcss/dist/low.css www/",
        "clean": "rm -rf ./www",
        "dev": "yarn clean && webpack serve",
        "serve": "serve ./www",
        "test": "jest"
    },
    "dependencies": {
        "@josemi-icons/react": "^0.3.2",
        "browser-fs-access": "^0.33.1",
        "classnames": "^2.3.2",
        "idb-keyval": "^6.2.0",
        "lowcss": "^0.13.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "uid": "^2.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.18.0",
        "@babel/plugin-transform-react-jsx": "^7.17.12",
        "@babel/plugin-transform-runtime": "^7.18.0",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@testing-library/react": "^14.0.0",
        "babel-jest": "^28.1.3",
        "babel-loader": "^9.1.2",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "mini-css-extract-plugin": "^2.7.2",
        "react": "^18.2.0",
        "react-test-renderer": "^18.2.0",
        "serve": "^14.2.1",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.11.1"
    },
    "license": "MIT",
    "jest": {
        "testEnvironment": "jsdom"
    }
